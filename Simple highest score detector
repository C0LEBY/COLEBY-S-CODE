student_scores = input("Please insert student scores and separate them with a space: ").split()

for scores in range(0, len(student_scores)):
    student_scores[scores] = int(student_scores[scores])

highest_score = 0
for score in student_scores:
    if score > highest_score:
        highest_score = score

print(f"The Highest score in the class is: {highest_score}")
